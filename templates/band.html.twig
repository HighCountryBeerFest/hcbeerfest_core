{#
/**
 * @file band.html.twig
 * Default theme implementation to present Band data.
 *
 * This template is used when viewing Band pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_band()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('band') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
